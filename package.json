{
  "name": "ileapapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "server": "ts-node server/index.ts",
    "server:dev": "nodemon --watch server --exec ts-node server/index.ts",
    "server:build": "tsc --project server/tsconfig.json",
    "dev": "concurrently \"npm run server:dev\" \"npm start\" --names \"API,EXPO\" --prefix-colors \"blue,green\"",
    "dev:all": "concurrently \"npm run server:dev\" \"npm start\" --names \"API,EXPO\" --prefix-colors \"blue,green\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/native-stack": "^7.6.2",
    "@tanstack/react-query": "^5.90.7",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "expo": "~54.0.24",
    "expo-notifications": "~0.32.13",
    "expo-status-bar": "~3.0.8",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react-native": "^0.553.0",
    "mongoose": "^8.19.3",
    "multer": "^1.4.5-lts.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.10.0",
    "@types/react": "~19.1.0",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
